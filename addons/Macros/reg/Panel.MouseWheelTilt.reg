REGEDIT4

; Usefull for mouse like http://www.logitech.com/repository/192/jpg/2110.1.0.jpg

; Tilt mouse wheel Left
[-HKEY_CURRENT_USER\Software\Far2\KeyMacros\Shell\MsM2Click]
[HKEY_CURRENT_USER\Software\Far2\KeyMacros\Shell\MsM2Click]
"Sequence"="CtrlPgUp"
"DisableOutput"=dword:00000001

; Tilt mouse wheel Right
[-HKEY_CURRENT_USER\Software\Far2\KeyMacros\Shell\MsM3Click]
[HKEY_CURRENT_USER\Software\Far2\KeyMacros\Shell\MsM3Click]
"Sequence"="CtrlPgDn"
"DisableOutput"=dword:00000001
